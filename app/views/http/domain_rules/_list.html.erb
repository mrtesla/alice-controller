<div class="span10">

  <table class="zebra-striped">
    <thead>
      <tr>
        <th>Domain</th>
        <th>Actions</th>
      </tr>
    </thead>

    <tbody>
      <% http_domain_rules.each do |http_domain_rule| %>
        <tr>
          <td><%= link_to "#{http_domain_rule.core_application.name}:#{http_domain_rule.domain}", http_domain_rule %></td>
          <td><%= http_domain_rule.actions %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
<div class="span4">
  <%#= link_to 'Edit', edit_core_machine_path(@core_machine), class: 'btn large' %>
</div>
