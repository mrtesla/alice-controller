<%= semantic_form_for [@core_application, @http_domain_rule] do |f| %>
  <%= f.semantic_errors %>
  <%= f.inputs do %>
    <%= f.input :core_application %>
    <%= f.input :domain %>
    <%= f.input :actions, as: :text %>
  <% end %>
  <%= f.buttons do %>
    <%= f.commit_button :button_html => {:class => "primary"} %>
  <% end %>
<% end %>

